{
  "name": "tyche",
  "version": "0.1.0",
  "description": "Browser-based Bayesian inference library with GPU acceleration",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./distributions": {
      "import": "./dist/distributions/index.js",
      "types": "./dist/distributions/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:ab": "cross-env DEMO=ab-test vite",
    "dev:combined": "cross-env DEMO=combined vite",
    "dev:explorer": "cross-env DEMO=inference-explorer vite",
    "dev:inference": "cross-env DEMO=inference-explorer vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "bayesian",
    "statistics",
    "mcmc",
    "inference",
    "webgl",
    "gpu",
    "probabilistic-programming"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/random-js": "^1.0.31",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "d3": "^7.9.0",
    "gamma": "^1.0.0",
    "jstat": "^1.9.6",
    "math-erf": "^1.0.0",
    "random-js": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0"
  }
}
